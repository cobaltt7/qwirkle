{
	"name": "scradd",
	"version": "2.4.0",
	"private": true,
	"type": "module",
	"main": "src/js/server.js",
	"scripts": {
		"build": "tsc --project tsconfig.json",
		"dev": "tsc --project tsconfig.json --watch",
		"format": "prettier --write \"**\" \"!src/js/**\" --ignore-unknown",
		"serve": "nodemon . --watch src",
		"start": "node ."
	},
	"prettier": "@redguy12/prettier-config",
	"dependencies": {
		"serve-handler": "6.1.5",
		"socket.io": "4.5.4"
	},
	"devDependencies": {
		"@redguy12/prettier-config": "2.0.2",
		"@types/serve-handler": "6.1.1",
		"nodemon": "2.0.20",
		"prettier": "2.8.0",
		"socket.io-client": "4.5.4",
		"typescript": "4.9.4"
	},
	"engines": {
		"node": "18.12.1",
		"npm": "8.13.2"
	}
}
