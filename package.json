{
	"name": "qwirkle",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"main": "src/js/server.js",
	"scripts": {
		"build": "npm run build:client && npm run build:server",
		"build:client": "vite build --outDir dist",
		"build:server": "tsc --project src/server/tsconfig.json",
		"format": "prettier --write \"**\" \"!dist/**\" --ignore-unknown",
		"serve": "npm run build:server -- --watch & nodemon dist/server --watch dist/server",
		"start": "node dist/server",
		"types": "vue-tsc --project tsconfig.json"
	},
	"browserslist": "defaults and supports es6-module and supports es6-module-dynamic-import",
	"prettier": "@redguy12/prettier-config",
	"dependencies": {
		"@vue/server-renderer": "3.2.47",
		"modern-normalize": "1.1.0",
		"serve-handler": "6.1.5",
		"socket.io": "4.5.4",
		"socket.io-client": "4.5.4",
		"twemoji": "14.0.2",
		"vue": "3.2.47",
		"vue-class-component": "8.0.0-rc.1"
	},
	"devDependencies": {
		"@redguy12/prettier-config": "2.0.2",
		"@types/serve-handler": "6.1.1",
		"@vitejs/plugin-vue": "4.0.0",
		"autoprefixer": "10.4.2",
		"compression": "1.7.4",
		"cssnano": "5.0.15",
		"htmlnano": "2.0.0",
		"nodemon": "2.0.20",
		"postcss": "8.4.5",
		"posthtml": "0.16.5",
		"prettier": "2.8.3",
		"terser": "5.16.4",
		"typescript": "4.9.4",
		"vite": "4.1.4",
		"vue-tsc": "1.1.5"
	},
	"engines": {
		"node": "18.12.1",
		"npm": "8.13.2"
	},"nodemonConfig":{}
}
